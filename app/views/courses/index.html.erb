<div class="row">
<div class="span6">
<h1>Materii</h1>
<table id="materii" class="table table-condensed ">
  <tr>
    <th>Nume</th>
    <th>Profesor</th>
    <th></th>
  </tr>

<% @courses.each do |course| %>
  <tr>
    <td><%= course.name %></td>
    <td><%= course.profesor %></td>
    <td><%= link_to 'Gestionare', course ,method: 'get',:remote=>true, class: 'btn btn-primary' %></td>
  </tr>
<% end %>
</table>
<br />
</div>
<div class="span6" id="courses_ajax">
</div>
</div>
